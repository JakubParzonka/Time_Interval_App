<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_menu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.jparzonka.time_interval_app.MenuActivity">




        <TextView
            android:id="@+id/info_device_textview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="DEVICE INFO"
            android:onClick="infoDeviceTextViewOnClick"/>

        <TextView
            android:id="@+id/LOREM_IPSUM_1_textview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="LOREM_IPSUM_1"
            android:onClick="lorem_ipsum_1_TextViewOnClick"
            android:layout_below="@+id/info_device_textview"/>

        <TextView
            android:id="@+id/LOREM_IPSUM_2_textview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="LOREM_IPSUM_2"
            android:layout_below="@+id/LOREM_IPSUM_1_textview" />


</RelativeLayout>
